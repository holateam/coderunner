{
  "name": "Coderunner service",
  "version": "0.0.1",
  "taskQueueLimit": 100,
  "MaxWorkingTaskNumber": 10,
  "requestAnalyticsTime": 60000,
  "dockerLogsLines": 50,
  "dockerLogsLineBytes": 256,
  "dockerSharedDir": "/home/vagrant/dockerShared",
  "dockerDir": "/home/vagrant/coderunner/docker",
  "serverSecret": "key",
  "supportedLangs": [
    "cpp"
  ],
  "userQuotes": {
    "taskLifetime": 3,
    "dockerMaxCores": 1,
    "dockerMaxMemory": 512
  },
  "quotes": {
    "maxTestCases": 2,
    "tasksPerMinute": 10,
    "rageCoefficient": 1.5,
    "codeLength": 1500,
    "dockerMinMemory": 128,
    "includes": {
      "acceptedCpp": {
        "#include<iostream>": true,
        "#include<stdlib.h>": true,
        "#include<string>": true,
        "#include<vector>": true
      }
    },
    "alarmObservers": [
      "mail@example.com",
      "somemail@domain.net"
    ]
  }
}